{
  "manifest_version": 2,
  "name": "Snag Time",
  "description": "Snags you the first available delivery time",
  "version": "1.0",
  "author": "Erin Brown",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "browser_action": {},
  "permissions": []
}